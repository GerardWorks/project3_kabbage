<hr>
<h1>dashboard</h1>
<hr>
<h3>trips</h3>
<% @trips.each do |trip| %>
  <% if trip.user_id == session[:id] %>
    <div><a href="/trip/<%=trip.id%>"><%=trip.name%></a></div>
  <% end %>
<% end %>

<hr>
<h3>Cards All</h3>
<% @cards.each do |card| %>
<p><%=card.body %></p>
<% end %>

<%= form_tag '/new_card_form', method: 'get' do %>
  <button>create new card</button>
<% end %>

<hr>
<a href="/mood_board">Mood Board</a>
